{
    "name": "igniterlabs/ti-ext-giftup",
    "type": "tastyigniter-package",
    "description": "Sell and accept gift cards on your TastyIgniter website using Gift Up!",
    "license": "MIT",
    "authors": [
        {
            "name": "Igniter Labs Team"
        }
    ],
    "keywords": [
        "tastyigniter",
        "giftup",
        "gift-card",
        "giftcard"
    ],
    "require": {
        "tastyigniter/core": "dev-master",
        "tastyigniter/ti-ext-cart": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "nunomaduro/larastan": "^2.4.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "IgniterLabs\\GiftUp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IgniterLabs\\GiftUp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "tastyigniter-extension": {
            "code": "igniterlabs.giftup",
            "name": "Gift Up! Gift Cards",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fc0",
                "color": "#000000"
            },
            "homepage": "https://tastyigniter.com/marketplace/item/igniterlabs-giftup"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "static": "vendor/bin/phpstan analyse --ansi --memory-limit 1056M"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "composer/installers": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
